Algoritmo "exemplo_notas"

Var
        nota1, nota2, total : real

Inicio
   Escreva("Digite a primeira nota:")
   leia(nota1)
   Escreva("Digite a segunda nota: ")
   leia(nota2)
   total <- nota1+nota2
   
   Escreval("Nota final = " , total:5:1)
   
   se total < 60 entao
      escreval ("REPROVADO")
  FIMSE


Fimalgoritmo